  port                 = 80
  protocol             = ""
  vpc_id               = "\$\{aws_vpc.${2:foo}.id\}"
  name                 = "${1:name}"
  name_prefix          = "" # Conflicts with name
  deregistration_delay = 300
  stickiness \{
    enabled         = true|false
    type            = "lb_cookie"
    cookie_duration = 1
  \}
  health_check \{
    interval            = 30
    path                = "/"
    port                = ""
    protocol            = "HTTP"
    timeout             = 5
    healthy_threshold   = 5
    matcher             = "200"
    unhealthy_threshold = 2
  \}
  tags \{
    TagName = ""
  \}
